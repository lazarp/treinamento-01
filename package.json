{
  "name": "treinamento-01",
  "version": "1.0.0",
  "description": "CAP Project on top of an existing HANA DB Project.",
  "repository": "<Add your repository here>",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "express": "^4.18.2",
    "passport": "^0.6.0",
    "body-parser": "^1.20.1",
    "hdb": "^0.19.5",
    "@sap/hdbext": "^7.7.0",
    "@sap/cds": "^6.4.0",
    "@sap/cds-odata-v2-adapter-proxy": "^1.9.16",
    "@sap/xsenv": "^3.4.0",
    "@sap/xssec": "^3.2.14",
    "@sap/hana-client": "^2.15.17"
  },
  "engines": {
    "node": "^16"
  },
  "scripts": {
    "start": "cds serve srv/gen/srv/csn.json"
  },
  "cds": {
    "build": {
      "target": "."
    },
    "hana": {
      "syntax": "hdi",
      "deploy-format": "hdbtable"
    },
    "requires": {
      "db": {
        "kind": "hana-cloud"
      },
      "auth": {
        "[production]": {
          "kind": "xsuaa"
        },
        "[development]":{
          "kind": "basic"
        }
      },
      "approuter": {
        "kind": "cloudfoundry"
      }
    }
  }
}